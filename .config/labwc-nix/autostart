# wlr-randr --output Virtual-1 --on --mode 1600x900@60 &
# top panel
waybar --log-level off &
# Set random wallpaper from ~/Pictures/Wallpapers
waypaper --random &
# Session Lock from https://labwc.github.io/integration.html#session-lock
swayidle -w \
timeout 300 'swaylock -f' \
timeout 600 'wlopm --off \*' \
resume 'wlopm --on \*' \
before-sleep 'swaylock -f' >/dev/null 2>&1 &
# Night Light with Gammastep
gammastep-indicator &
